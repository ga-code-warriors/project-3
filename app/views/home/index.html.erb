
<h1>Books</h1>

<%= link_to 'New Book', new_book_path %>

<table>
  <thead>
    <tr>
      <th>Title</th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td> 
        <td><%= link_to 'Show', book %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>


      